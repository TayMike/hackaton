{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Paciente",
  "type": "object",
  "properties": {
    "id": { "type": "string", "format": "uuid" },
    "cpf": { "type": "string", "pattern": "^\\d{11}$" },
    "nome": { "type": "string", "minLength": 1, "maxLength": 100 },
    "dataNascimento": { "type": ["string", "null"], "format": "date-time" },
    "primeiroDiaCadastro": { "type": "string", "format": "date-time" },
    "cep": { "type": "string", "pattern": "^\\d{8}$" },
    "numeroCasa": { "type": "integer", "minimum": 1 }
  },
  "required": [
    "cpf",
    "nome",
    "primeiroDiaCadastro",
    "cep",
    "numeroCasa"
  ]
}