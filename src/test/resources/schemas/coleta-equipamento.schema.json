{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ColetaEquipamento",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "equipamentoIds": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      }
    },
    "quantidades": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 0
      }
    },
    "colaboradorEntregadorId": {
      "type": "string",
      "format": "uuid"
    },
    "dataHoraColeta": {
      "type": "string",
      "format": "date-time"
    },
    "colaboradorResponsavelId": {
      "type": "string",
      "format": "uuid"
    },
    "hospitalId": {
      "type": "string",
      "format": "uuid"
    }
  },
  "required": [
    "equipamentos",
    "quantidades",
    "colaboradorEntregador",
    "dataHoraColeta",
    "colaboradorResponsavel",
    "hospital"
  ]
}