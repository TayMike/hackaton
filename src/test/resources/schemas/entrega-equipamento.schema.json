{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "EntregaEquipamento (IDs Only)",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "equipamentos": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      }
    },
    "quantidade": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 0
      }
    },
    "colaboradorRecebedor": {
      "type": "string",
      "format": "uuid"
    },
    "dataHoraRecebimento": {
      "type": "string",
      "format": "date-time"
    },
    "hospital": {
      "type": "string",
      "format": "uuid"
    }
  },
  "required": [
    "equipamentos",
    "quantidade",
    "colaboradorRecebedor",
    "dataHoraRecebimento",
    "hospital"
  ]
}