{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Colaborador",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "cpf": {
      "type": "string"
    },
    "nome": {
      "type": "string"
    },
    "matricula": {
      "type": "string"
    },
    "primeiroDiaCadastro": {
      "type": "string",
      "format": "date-time"
    },
    "cep": {
      "type": "string"
    },
    "numeroCasa": {
      "type": "integer"
    },
    "hospitalId": {
      "type": "string",
      "format": "uuid"
    },
    "setor": {
      "type": "string"
    },
    "ativo": {
      "type": "boolean"
    }
  },
  "required": [
    "cpf",
    "nome",
    "matricula",
    "primeiroDiaCadastro",
    "cep",
    "numeroCasa",
    "hospital",
    "setor",
    "ativo"
  ]
}