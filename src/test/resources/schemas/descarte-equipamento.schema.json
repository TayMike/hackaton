{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "DescarteEquipamento (API Response)",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "equipamentos": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uuid"
      }
    },
    "quantidade": {
      "type": "array",
      "items": {
        "type": "integer",
        "minimum": 0
      }
    },
    "colaboradorResponsavel": {
      "type": "string",
      "format": "uuid"
    },
    "dataHoraDescarte": {
      "type": "string",
      "format": "date-time"
    },
    "hospital": {
      "type": "string",
      "format": "uuid"
    }
  },
  "required": [
    "equipamentos",
    "quantidade",
    "colaboradorResponsavel",
    "dataHoraDescarte",
    "hospital"
  ]
}