{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ColetaInsumo (Objetos)",
  "type": "object",
  "properties": {
    "id": {
      "type": "string",
      "format": "uuid"
    },
    "insumos": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": { "type": "string", "format": "uuid" }
        },
        "required": ["id"]
      }
    },
    "quantidades": {
      "type": "array",
      "items": { "type": "integer", "minimum": 0 }
    },
    "colaboradorEntregador": { "type": "string", "format": "uuid" },
    "dataHoraColeta": { "type": "string", "format": "date-time" },
    "pacienteRecebedor": { "type": "string", "format": "uuid" },
    "hospital": {
      "type": "object",
      "properties": {
        "id": { "type": "string", "format": "uuid" }
      },
      "required": ["id"]
    }
  },
  "required": [
    "insumos",
    "quantidades",
    "colaboradorEntregador",
    "dataHoraColeta",
    "pacienteRecebedor",
    "hospital"
  ]
}